class Rectangle {
    init(h, w) {
        this.h = h;
        this.w = w;
    }

    area() {
        return this.h * this.w;
    }
}

class Square < Rectangle {
    init(h) {
        this.h = h;
        this.w = h;
    }
}

var r = Rectangle(2, 4);
var s = Square(4);

print r.area();
print s.area();